# Left Wheel Copy
"xte 'keydown Control_L' 'key C' 'keyup Control_L'"
    release + b:6

# Right Wheel Paste
"xte 'keydown Control_L' 'key V' 'keyup Control_L'"
    release + b:7

# Unity - spread all windows in current workspace
"xte 'keydown Super_L' 'key W' 'keyup Super_L'"
    release + b:10

# Browser Refresh
"xte 'key F5'"
    release + b:8
